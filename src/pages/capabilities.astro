---
import BaseLayout from '../layouts/BaseLayout.astro';
import Navigation from '../components/Navigation';
import Breadcrumbs from '../components/Breadcrumbs.astro';
import CapabilitiesPage from '../page-components/Capabilities.tsx';
---

<BaseLayout 
  title="Our Capabilities | Make Good Melbourne"
  description="Comprehensive make good services for commercial and industrial properties. Office strip outs, warehouse restoration, flooring, painting, cleaning and more."
  canonicalUrl="https://makegood.melbourne/capabilities"
>
  <Navigation client:load />
  <div class="bg-secondary pt-20">
    <Breadcrumbs path="/capabilities" />
  </div>
  <CapabilitiesPage client:idle />
</BaseLayout>
