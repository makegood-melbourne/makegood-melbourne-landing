---
import BaseLayout from '../layouts/BaseLayout.astro';
import Navigation from '../components/Navigation';
import HeroSection from '../components/HeroSection.astro';
import AboutSection from '../components/AboutSection.astro';
import IndexContent from '../components/IndexContent';
---

<BaseLayout 
  title="Commercial & Warehouse Make Good Solutions Melbourne"
  description="Commercial & industrial make good solutions in Melbourne. End of lease restoration for offices, warehouses and industrial sites. Free quotes."
  ogImage="/og/commercial.jpg"
  canonicalUrl="https://makegood.melbourne/"
>
  <main class="min-h-screen bg-background text-foreground">
    <Navigation client:load />
    <HeroSection />
    <IndexContent client:idle />
    <AboutSection />
  </main>
</BaseLayout>
